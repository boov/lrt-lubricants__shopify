<meta property="og:locale" content="en_GB">
<meta property="og:type" content="website">
<meta property="og:title" content="{{ title | default: seo.title }} - {{ company.name }}">
<meta property="og:description" content="{{ description | default: seo.description }}">
<meta property="og:url" content="{{ app.url }}">
<meta property="og:site_name" content="{{ company.name }}">
<meta property="og:image" content="{{ featured_image | default: '/assets/social_share.png' }}">
<meta property="og:image:secure_url" content="{{ featured_image | default: '/assets/social_share.png' }}">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="670">
